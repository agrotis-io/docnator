{
  "name": "star-t",
  "version": "0.0.1",
  "description": "Start node module",
  "license": "MIT",
  "repository": "github.com/agrotis-io/star-t",
  "main": "index.js",
  "author": {
    "name": "JoÃ£o Henrique Serodio",
    "email": "jhserodio@gmail.com",
    "url": "http://github.com/jhserodio"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "build": "babel src -d dist",
    "coverage": "npm test -- --coverage",
    "lint": "eslint src test",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "precommit": "lint-staged",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "jest",
    "watch": "npm-watch"
  },
  "lint-staged": {
    "*.js": "prettier -c .prettierrc src/** --write"
  },
  "watch": {
    "test": "{src,test}/*.js",
    "lint": "{src,test}/*.js",
    "build": "src"
  },
  "release": {
    "publish": [
      {
        "path": "@semantic-release/npm",
        "pkgRoot": "dist"
      },
      "@semantic-release/github"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "mapCoverage": true,
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "keywords": [
    "generator-nod"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "~6.18.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "~22.0.6",
    "babel-preset-env": "~1.1.8",
    "babel-preset-stage-2": "~6.18.0",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.9.6",
    "documentation": "~5.3.2",
    "husky": "^0.14.3",
    "jest-cli": "~22.0.6",
    "lint-staged": "^6.1.0",
    "npm-watch": "~0.3.0",
    "opn-cli": "~3.1.0",
    "prettier": "1.10.2",
    "rimraf": "~2.6.1",
    "semantic-release": "^12.4.1"
  }
}
